    <TargetFramework>net5.0-windows</TargetFramework>
    <UseWPF>true</UseWPF>
    <Version>1.2.1</Version>
    <Authors>Weedeej</Authors>
    <Company>N/A</Company>
    <ApplicationIcon>Resources\vtools-colored.ico</ApplicationIcon>
    <PackageIcon></PackageIcon>
    <PackageIconUrl />
    <StartupObject />
  </PropertyGroup>

  <ItemGroup>
    <None Remove="crosshairBg0.png" />
